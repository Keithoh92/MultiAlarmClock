CREATE TABLE alarmClocks {
    id INTEGER PRIMARY KEY,
    alarmName TEXT NOT NULL,
    daysSelected TEXT NOT NULL,
    weekly BOOLEAN,
    startTime TEXT NOT NULL,
    endTime TEXT NOT NULL,
    sound TEXT NOT NULL,
    intervalTime INTEGER,
    timeAdded TEXT NOT NULL
};

CREATE INDEX alarm_clock_id ON alarmClocks(id)

